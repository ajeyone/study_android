<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".RxJavaActivity">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <Button
                android:id="@+id/button_takeWhile"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="takeWhile()"
                android:textAllCaps="false" />
            <Button
                android:id="@+id/button_all"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="all()"
                android:textAllCaps="false" />

            <Button
                android:id="@+id/button_repeat"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="repeat()"
                android:textAllCaps="false" />

            <Button
                android:id="@+id/button_amb"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="amb()"
                android:textAllCaps="false" />

            <Button
                android:id="@+id/button_debounce"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="debounce()"
                android:textAllCaps="false" />

            <Button
                android:id="@+id/button_filter"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="filter()"
                android:textAllCaps="false" />

            <Button
                android:id="@+id/button_observeOn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="observeOn()"
                android:textAllCaps="false" />

            <Button
                android:id="@+id/button17"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="combineLatest()"
                android:textAllCaps="false" />

            <Button
                android:id="@+id/button16"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="zip()"
                android:textAllCaps="false" />

            <Button
                android:id="@+id/button1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="create()"
                android:textAllCaps="false" />

            <Button
                android:id="@+id/button2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="just()"
                android:textAllCaps="false" />

            <Button
                android:id="@+id/button3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="fromArray()"
                android:textAllCaps="false" />

            <Button
                android:id="@+id/button4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="fromCallable()"
                android:textAllCaps="false" />

            <Button
                android:id="@+id/button5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="fromFuture()"
                android:textAllCaps="false" />

            <Button
                android:id="@+id/button6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="fromIterable()"
                android:textAllCaps="false" />

            <Button
                android:id="@+id/button7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Observable.defer()"
                android:textAllCaps="false" />


            <Button
                android:id="@+id/button8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Observable.timer()"
                android:textAllCaps="false" />

            <Button
                android:id="@+id/button9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="map()"
                android:textAllCaps="false" />

            <Button
                android:id="@+id/button10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="concatMap()"
                android:textAllCaps="false" />

            <Button
                android:id="@+id/button11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="buffer()"
                android:textAllCaps="false" />

            <Button
                android:id="@+id/button12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="groupBy()"
                android:textAllCaps="false" />

            <Button
                android:id="@+id/button13"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="scan() ..."
                android:textAllCaps="false" />

            <Button
                android:id="@+id/button14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="concat()"
                android:textAllCaps="false" />

            <Button
                android:id="@+id/button15"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="merge()"
                android:textAllCaps="false" />
        </LinearLayout>
    </ScrollView>
</FrameLayout>